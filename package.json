{
  "name": "sad-creeper",
  "version": "1.0.0",
  "private": true,
  "description": "A Probot app to check for a valid server version output in newly created issues.",
  "author": "Tim Brust <github@timbrust.de>",
  "license": "MIT",
  "repository": "https://github.com/timbru31/sad-creeper.git",
  "homepage": "https://github.com/timbru31/sad-creeper",
  "bugs": "https://github.com/timbru31/sad-creeper/issues",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc && (tsc -w --preserveWatchOutput & nodemon)",
    "dev": "npm run build:watch",
    "start": "npm run build && probot run ./lib/index.js",
    "lint": "standard **/*.ts --fix",
    "test": "jest && standard **/*.ts",
    "test:watch": "jest --watch --notify --notifyMode=change --coverage"
  },
  "dependencies": {
    "probot": "^10.17.3"
  },
  "devDependencies": {
    "@now/node": "^1.8.5",
    "@types/jest": "^26.0.16",
    "@types/nock": "^11.1.0",
    "@types/node": "^14.14.10",
    "@typescript-eslint/parser": "^4.9.0",
    "eslint-plugin-typescript": "^0.14.0",
    "jest": "^26.6.3",
    "nock": "^13.0.5",
    "nodemon": "^2.0.6",
    "smee-client": "^1.2.2",
    "standard": "^16.0.3",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.2"
  },
  "engines": {
    "node": ">= 10.16"
  },
  "standard": {
    "parser": "@typescript-eslint/parser",
    "env": [
      "jest"
    ],
    "plugins": [
      "typescript"
    ]
  },
  "jest": {
    "testEnvironment": "node"
  }
}
