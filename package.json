{
  "name": "sad-creeper",
  "version": "1.0.0",
  "private": true,
  "description": "A Probot app to check for a valid server version output in newly created issues.",
  "author": "Tim Brust <github@timbrust.de>",
  "license": "MIT",
  "repository": "https://github.com/timbru31/sad-creeper.git",
  "homepage": "https://github.com/timbru31/sad-creeper",
  "bugs": "https://github.com/timbru31/sad-creeper/issues",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc && (tsc -w --preserveWatchOutput & nodemon)",
    "dev": "npm run build:watch",
    "start": "npm run build && probot run ./lib/index.js",
    "lint": "standard **/*.ts --fix",
    "test": "jest && standard **/*.ts",
    "test:watch": "jest --watch --notify --notifyMode=change --coverage"
  },
  "dependencies": {
    "@probot/get-private-key": "^1.1.1",
    "probot": "^12.1.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "@types/node": "^14.17.10",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "@vercel/node": "^1.12.1",
    "jest": "^26.6.3",
    "nock": "^13.1.2",
    "nodemon": "^2.0.12",
    "smee-client": "^1.2.2",
    "standard": "^16.0.3",
    "ts-jest": "^26.5.6",
    "typescript": "^4.3.5"
  },
  "standard": {
    "parser": "@typescript-eslint/parser",
    "env": [
      "jest"
    ],
    "plugins": [
      "@typescript-eslint"
    ]
  },
  "jest": {
    "testEnvironment": "node"
  }
}
